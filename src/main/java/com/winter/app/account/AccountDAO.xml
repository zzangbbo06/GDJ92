<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!-- namespace의 이름은 사용하려는 DAO의 풀패키지명과 동일하게 작성 -->
  <mapper namespace="com.winter.app.account.AccountDAO">
	<insert id="add" parameterType="java.util.List">
		INSERT INTO ACCOUNT VALUES
		<foreach collection="list" item="account" open="" separator="," close="">
			(#{account.accountNum}, #{account.username}, #{account.productNum}, 0, now())
		</foreach>
	</insert>
  </mapper>
  